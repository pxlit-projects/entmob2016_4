<Application
    x:Class="EntMob_Uni.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EntMob_Uni"
    xmlns:converter="using:EntMob_Uni.Converter"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <converter:ValueToDegreesConverter x:Key="localValueToDegreesConverter"></converter:ValueToDegreesConverter>
            <converter:ValueToKmConverter x:Key="localValueToKmConverter"></converter:ValueToKmConverter>
            <converter:TimeSpanConverter x:Key="localTimeSpanConverter"></converter:TimeSpanConverter>
            <converter:DateTimeConverter x:Key="localDateTimeConverter"></converter:DateTimeConverter>
            <local:ViewModelLocator x:Key="mainViewModelLocator"></local:ViewModelLocator>
        </ResourceDictionary>
    </Application.Resources>
</Application>
